#: To_Lands_Unknown/scenarios/13_Far_North.cfg:40
msgid ""
"You can always turn on/off some weather effects. To do that, right-click "
"anywhere on the map and choose which effects you want to disable/enable."
msgstr ""

#. [side]: type=EoMa_Platinum_Warrior, id=Dedylos
#: To_Lands_Unknown/scenarios/13_Far_North.cfg:70
#: To_Lands_Unknown/scenarios/14_Gates_of_Kharos.cfg:137
msgid "Dedylos"
msgstr "Dedylos"

#. [side]
#: To_Lands_Unknown/scenarios/13_Far_North.cfg:92
msgid "team_name^Hydras"
msgstr "Idre"

#. [side]
#: To_Lands_Unknown/scenarios/13_Far_North.cfg:105
msgid "team_name^Invaders"
msgstr "Invasori"

