#: To_Lands_Unknown/scenarios/06_Ka-gatta.cfg:420
msgid "ancient cuirass"
msgstr "corazza antica"

#. [value]: id=cota
#: To_Lands_Unknown/scenarios/06_Ka-gatta.cfg:421
